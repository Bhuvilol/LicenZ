# LicenZ Environment Configuration
# Copy this file to .env and fill in your actual API keys

# AI Generation Services
VITE_STABILITY_API_KEY=your_stability_api_key_here
VITE_OPENAI_API_KEY=your_openai_api_key_here
VITE_REPLICATE_API_KEY=your_replicat_api_key_here

# Blockchain & NFT Services
VITE_VERBWIRE_API_KEY=your_verbwire_api_key_here
VITE_INFURA_API_KEY=your_infura_api_key_here

# IPFS Services (Optional)
VITE_PINATA_API_KEY=your_pinata_api_key_here
VITE_PINATA_SECRET=your_pinata_secret_here

# Backend Configuration (if using backend)
VITE_BACKEND_URL=http://localhost:8080

# Note: All environment variables must start with VITE_ to be accessible in the browser
# This is a Vite requirement for security reasons

